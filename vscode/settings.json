{
    "editor.wordWrap": "on",
    "git.autofetch": true,
    "explorer.confirmDelete": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "explorer.confirmDragAndDrop": false,
    "git.confirmSync": false,
    "python.pythonPath": "/Applications/Xcode.app/Contents/Developer/usr/bin/python3",
    "python.formatting.provider": "black",
    "workbench.iconTheme": "vscode-icons",
    "vsicons.dontShowNewVersionMessage": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off"
    },
    "dart.previewLsp": true,
    "dart.openDevTools": "flutter",
    "editor.fontSize": 17,
    "security.workspace.trust.untrustedFiles": "open",
    "notebook.lineNumbers": "on",
    "workbench.colorTheme": "Tokyo Night",
    "python.defaultInterpreterPath": "/usr/bin/python3",
    "platformio-ide.autoOpenPlatformIOIniFile": false,
    "auto-build.defaultEnv.name": "mega2560",
    "auto-build.build.reveal": false,
    "editor.inlineSuggest.suppressSuggestions": true,
    "python.createEnvironment.trigger": "off",
    "workbench.editor.empty.hint": "hidden",
    "terminal.integrated.commandsToSkipShell": [
        "matlab.interrupt"
    ],
    "MATLAB.installPath": "/Applications/MATLAB_R2024b.app",
    "[matlab]": {
        "editor.defaultFormatter": "MathWorks.language-matlab"
    },
    "editor.fontFamily": "Operator Mono, Menlo, Monaco, 'Courier New', monospace",
    "editor.lineHeight": 25,
    "editor.letterSpacing": 0.5,
    "files.trimTrailingWhitespace": true,
    "editor.fontWeight": "400",
    "editor.cursorStyle": "line",
    "editor.cursorWidth": 2,
    "editor.cursorBlinking": "smooth",
    "editor.renderWhitespace": "selection",
    "svelte.enable-ts-plugin": true,
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock",
        "*.sqlite": "${capture}.${extname}-*",
        "*.db": "${capture}.${extname}-*",
        "*.sqlite3": "${capture}.${extname}-*",
        "*.db3": "${capture}.${extname}-*",
        "*.sdb": "${capture}.${extname}-*",
        "*.s3db": "${capture}.${extname}-*"
    },
}